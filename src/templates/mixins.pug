-const subjects = [{"name":"Історія України"},{"name":"Біологія"},{"name":"Математика"},{"name":"Іноземна мова"},{"name":"Хімія"},{"name":"Географія/Іноземна мова"},{"name":"Хімія/Іноземна мова"},{"name":"Історія України/Іноземна мова"},{"name":"Математика/Іноземна мова"},{"name":"Фізика/Іноземна мова"},{"name":"Хімія/Географія"},{"name":"Історія/Математика"},{"name":"Біологія/Іноземна мова"},{"name":"Історія/Географія"}];
